---
version: '2'
services:
  {{ project_name }}:
    command: {{ development_command }}
    {%- if environment %}
    environment:
    {%- for e in environment %}
    - {{ e }}
    {% endfor -%}
    {% endif -%}
    extends:
      file: common.yml
      service: {{ project_name }}
    ports:
      - {{ port }}:{{ port }}
    volumes:
      - ../:/app
